<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Play_Mode">

    <RelativeLayout
        android:id="@+id/rel_all"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        tools:ignore="UselessParent">

        <RelativeLayout
            android:id="@+id/rel_light_gr"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_alignParentTop="true"
            android:background="#8CCB5E">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="900dp"
            android:layout_below="@+id/rel_light_gr"
            android:background="#228B22	">

        </RelativeLayout>

        <ScrollView
            android:id="@+id/scroll"
            android:layout_width="360dp"
            android:layout_height="match_parent"
            android:layout_below="@+id/txt_main_name"
            android:layout_centerHorizontal="true"
            android:scrollbars="none">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/rel_one"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/new_green"
                    android:clickable="true"
                    android:focusable="true">

                    <RelativeLayout
                        android:id="@+id/rel_small_white"
                        android:layout_width="3dp"
                        android:layout_height="90dp"
                        android:layout_alignBottom="@+id/ic_man_one"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/gra_white"
                        android:orientation="horizontal"
                        tools:ignore="DuplicateIds" />

                    <ImageView
                        android:id="@+id/ic_man_one"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_marginStart="40dp"
                        android:contentDescription="@string/todo"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp" />

                    <ImageView
                        android:id="@+id/ic_man_two"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginEnd="40dp"
                        android:layout_marginRight="40dp"
                        android:contentDescription="@string/todo"
                        android:paddingBottom="10dp"
                        android:paddingTop="10dp" />

                    <TextView
                        android:id="@+id/txt_oneone"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_alignParentTop="false"
                        android:layout_below="@+id/ic_man_one"
                        android:paddingTop="5dp"
                        android:text="@string/one_vs_one"
                        android:textAlignment="center"
                        android:textColor="#FFF"
                        android:textSize="24sp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_five"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_below="@+id/rel_two"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/new_green">

                    <RelativeLayout
                        android:id="@+id/rel_small_white_two"
                        android:layout_width="3dp"
                        android:layout_height="90dp"
                        android:layout_above="@+id/txt_comandcomand"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/gra_white"
                        android:orientation="horizontal" />

                    <ImageView
                        android:id="@+id/ic_woman_two"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_seven"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:layout_toLeftOf="@+id/ic_man_eight"
                        android:layout_toStartOf="@+id/ic_man_eight"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_six"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/ic_woman_two"
                        android:layout_toRightOf="@+id/ic_woman_two"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_eight"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <TextView
                        android:id="@+id/txt_comandcomand"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_alignParentTop="false"
                        android:layout_below="@+id/ic_woman_two"
                        android:paddingTop="5dp"
                        android:text="@string/comand_vs_comand"
                        android:textAlignment="center"
                        android:textColor="#FFF"
                        android:textSize="24sp"
                        android:textStyle="bold" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rel_two"
                    android:layout_width="350dp"
                    android:layout_height="150dp"
                    android:layout_below="@+id/rel_one"
                    android:layout_marginTop="19dp"
                    android:background="@drawable/new_green"
                    android:clickable="true"
                    android:focusable="true">

                    <ImageView
                        android:id="@+id/ic_woman_one"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_ten"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/ic_woman_one"
                        android:layout_toRightOf="@+id/ic_woman_one"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_four"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentTop="true"
                        android:layout_toLeftOf="@+id/ic_man_five"
                        android:layout_toStartOf="@+id/ic_man_five"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <ImageView
                        android:id="@+id/ic_man_five"
                        android:layout_width="85dp"
                        android:layout_height="100dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentTop="true"
                        android:contentDescription="@string/todo"
                        android:paddingTop="15dp" />

                    <TextView
                        android:id="@+id/txt_comandcom"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_alignParentTop="false"
                        android:layout_below="@+id/ic_woman_one"
                        android:paddingTop="5dp"
                        android:text="@string/comand"
                        android:textAlignment="center"
                        android:textColor="#FFF"
                        android:textSize="24sp"
                        android:textStyle="bold" />
                </RelativeLayout>

            </RelativeLayout>
        </ScrollView>

        <TextView
            android:id="@+id/txt_main_name"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingTop="70dp"
            android:text="@string/crocodile"
            android:textAlignment="center"
            android:textColor="#FFF"
            android:textSize="26sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_noisy"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingRight="250dp"
            android:paddingTop="70dp"
            android:text="@string/noisy"
            android:textAlignment="center"
            android:textColor="#FFF"
            android:textSize="12sp"
            android:textStyle="bold"
            android:paddingEnd="250dp"
            tools:ignore="RtlSymmetry" />

        <TextView
            android:id="@+id/txt_party"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingLeft="250dp"
            android:paddingTop="70dp"
            android:text="@string/party"
            android:textAlignment="center"
            android:textColor="#FFF"
            android:textSize="12sp"
            android:textStyle="bold"
            android:paddingStart="250dp"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/ic_crock_one"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingBottom="5dp"
            android:contentDescription="@string/todo"/>

        <ImageView
            android:id="@+id/ic_crock_two"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingBottom="30dp"
            android:paddingRight="250dp"
            android:paddingTop="30dp"
            android:contentDescription="@string/todo"
            android:paddingEnd="250dp"
            tools:ignore="RtlSymmetry" />

        <ImageView
            android:id="@+id/ic_crock_three"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:paddingBottom="30dp"
            android:paddingLeft="250dp"
            android:paddingTop="30dp"
            android:contentDescription="@string/todo"
            android:paddingStart="250dp"
            tools:ignore="RtlSymmetry" />

    </RelativeLayout>

</RelativeLayout>

















